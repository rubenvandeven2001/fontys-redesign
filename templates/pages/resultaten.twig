<div class="results__wrapper">
    <div class="results-table">
        <div class="results-table__col">
            <h2 class="h2">Vakken</h2>

            <div class="results-table__rows">
                {% for index in 1..6 %}
                    <div class="results-table__item">Media</div>
                {% endfor %}
            </div>
        </div>

        <div class="results-table__col results-table__col--span">
            <h2 class="h2">Feedback onderwerpen</h2>

            <div class="results-table__rows-scroll">
                {% for index in 1..9 %}
                    <div class="results-table__rows">
                        {% for index in 1..6 %}
                            <div class="results-table__item">
                                <span class="results-table__spacing">Zelfportret</span>
                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                                    <path d="M12 2c5.514 0 10 4.486 10 10s-4.486 10-10 10-10-4.486-10-10 4.486-10 10-10zm0-2c-6.627 0-12 5.373-12 12s5.373 12 12 12 12-5.373 12-12-5.373-12-12-12zm5.507 13.941c-1.512 1.195-3.174 1.931-5.506 1.931-2.334 0-3.996-.736-5.508-1.931l-.493.493c1.127 1.72 3.2 3.566 6.001 3.566 2.8 0 4.872-1.846 5.999-3.566l-.493-.493zm-9.007-5.941c-.828 0-1.5.671-1.5 1.5s.672 1.5 1.5 1.5 1.5-.671 1.5-1.5-.672-1.5-1.5-1.5zm7 0c-.828 0-1.5.671-1.5 1.5s.672 1.5 1.5 1.5 1.5-.671 1.5-1.5-.672-1.5-1.5-1.5z" />
                                </svg>
                            </div>
                        {% endfor %}
                    </div>
                {% endfor %}

                <div class="results-table__overlay"></div>
            </div>
        </div>

        <div class="results-table__col">
            <h2 class="h2">Totaal</h2>

            <div class="results-table__rows">
                {% for index in 1..6 %}
                    <div class="results-table__item">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path d="M12 2c5.514 0 10 4.486 10 10s-4.486 10-10 10-10-4.486-10-10 4.486-10 10-10zm0-2c-6.627 0-12 5.373-12 12s5.373 12 12 12 12-5.373 12-12-5.373-12-12-12zm5.507 13.941c-1.512 1.195-3.174 1.931-5.506 1.931-2.334 0-3.996-.736-5.508-1.931l-.493.493c1.127 1.72 3.2 3.566 6.001 3.566 2.8 0 4.872-1.846 5.999-3.566l-.493-.493zm-9.007-5.941c-.828 0-1.5.671-1.5 1.5s.672 1.5 1.5 1.5 1.5-.671 1.5-1.5-.672-1.5-1.5-1.5zm7 0c-.828 0-1.5.671-1.5 1.5s.672 1.5 1.5 1.5 1.5-.671 1.5-1.5-.672-1.5-1.5-1.5z" />
                        </svg>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>

    <div class="results__selected">
    
    </div>
</div>